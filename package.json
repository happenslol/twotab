{
  "name": "twotab",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "fix": "prettier -w .",
    "ext:start": "pnpm run build && web-ext run --source-dir ./dist",
